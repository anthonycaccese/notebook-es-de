<!-- 
16:9 aspect ratio
based on 853x480
-->
<theme>
   <view name="gamelist">
      <image name="table">
         <path>./_inc/images/16-9/layer-0-table-${tableImageColor}.png</path>
      </image>
      <image name="paper">
         <path>./_inc/images/16-9/layer-1-paper.png</path>
      </image>
      <image name="highlighter-color">
         <path>./_inc/images/16-9/layer-2-highlighter-color.png</path>
      </image>
      <image name="highlighter">
         <path>./_inc/images/16-9/layer-3-highlighter.png</path>
      </image>
      <image name="notepad">
         <path>./_inc/images/16-9/layer-4-notepad.png</path>
      </image>
      <image name="tab-yellow-off">
         <path>./_inc/images/16-9/layer-5-tab-yellow-off.png</path>
      </image>
      <badges name="tab-yellow-on">
         <size>1 1</size>
         <lines>1</lines>
         <itemsPerLine>1</itemsPerLine>
         <slots>favorite</slots>
         <itemMargin>0 0</itemMargin>
         <customBadgeIcon badge="favorite">./_inc/images/16-9/layer-6-tab-yellow-on.png</customBadgeIcon>
         <zIndex>6</zIndex>
      </badges>
      <image name="tab-blue-off">
         <path>./_inc/images/16-9/layer-7-tab-blue-off.png</path>
      </image>
      <badges name="tab-blue-on">
         <size>1 1</size>
         <lines>1</lines>
         <itemsPerLine>1</itemsPerLine>
         <slots>altemulator</slots>
         <itemMargin>0 0</itemMargin>
         <customBadgeIcon badge="altemulator">./_inc/images/16-9/layer-8-tab-blue-on.png</customBadgeIcon>
         <zIndex>8</zIndex>
      </badges>
      <image name="tab-green-off">
         <path>./_inc/images/16-9/layer-9-tab-green-off.png</path>
      </image>
      <badges name="tab-green-on">
         <size>1 1</size>
         <lines>1</lines>
         <itemsPerLine>1</itemsPerLine>
         <slots>completed</slots>
         <itemMargin>0 0</itemMargin>
         <customBadgeIcon badge="completed">./_inc/images/16-9/layer-10-tab-green-on.png</customBadgeIcon>
         <zIndex>10</zIndex>
      </badges>
      <image name="tab-red-off">
         <path>./_inc/images/16-9/layer-11-tab-red-off.png</path>
      </image>
      <badges name="tab-red-on">
         <size>1 1</size>
         <lines>1</lines>
         <itemsPerLine>1</itemsPerLine>
         <slots>broken</slots>
         <itemMargin>0 0</itemMargin>
         <customBadgeIcon badge="broken">./_inc/images/16-9/layer-12-tab-red-on.png</customBadgeIcon>
         <zIndex>12</zIndex>
      </badges>
      <image name="polaroid-background">
         <pos>0.71746776084408 0.333333333333333</pos><!-- 612 160 -->
         <cropSize>0.304806565064478 0.5625</cropSize><!-- 260 270 -->
         <rotation>11</rotation>
      </image>
      <textlist name="list">
         <pos>0.206330597889801 0.177083333333333</pos><!-- 176 85 -->
         <!-- Without Logo <pos>0.206330597889801 0.133333333333333</pos><! 176 64 -->
         <size>0.246189917936694 0.604166666666667</size><!-- 210 290 -->
         <horizontalMargin>0.025791324736225</horizontalMargin><!-- 22 -->
         <selectorVerticalOffset>0.003</selectorVerticalOffset><!-- 1.44 -->
      </textlist>
      <video name="polaroid-art">
         <pos>0.71746776084408 0.333333333333333</pos><!-- 612 160 -->
         <cropSize>0.304806565064478 0.5625</cropSize><!-- 260 270 -->
         <rotation>11</rotation>
      </video>
      <image name="polaroid">
         <path>./_inc/images/16-9/layer-15-polaroid.png</path>
      </image>
      <image name="pen">
         <path>./_inc/images/16-9/layer-18-pen.png</path>
      </image>
      <image name="logo">
         <pos>0.328253223915592 0.120833333333333</pos><!-- 280 58 -->
         <maxSize>0.18757327080891 0.045833333333333</maxSize><!-- 160 22 -->
      </image>
      <image name="logo-distort">
         <path>./_inc/images/16-9/layer-20-logo-distort.png</path>
      </image>
      <helpsystem name="help">
         <pos>0.248534583821805 0.933333333333333</pos><!-- 212 448 -->
      </helpsystem>
   </view>
   <variant name="gamelist-screenshot,gamelist-fanart">
      <view name="gamelist">
         <image name="sticker-glow,sticker">
            <maxSize>0.199296600234467 0.166666666666667</maxSize><!-- 170 80 -->
            <rotation>11</rotation>
         </image>
         <image name="sticker-glow">
            <pos>0.763188745603751 0.63125</pos><!-- 651 303 -->
         </image>
         <image name="sticker">
            <pos>0.762016412661196 0.629166666666667</pos><!-- 650 302 -->
         </image>
      </view>
   </variant>
</theme>