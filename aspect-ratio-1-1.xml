      </video>
      <image name="polaroid">
         <path>./_inc/images/1-1/layer-15-polaroid.png</path>
      </image>
      <image name="logo">
         <pos>0.270833333333333 0.120833333333333</pos><!-- 130 58 -->
         <maxSize>0.333333333333333 0.045833333333333</maxSize><!-- 160 22 -->
